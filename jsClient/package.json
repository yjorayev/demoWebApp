{
  "name": "jsClient",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build:watch": "webpack -d --watch --color",
    "build": "SET NODE_ENV=development && webpack -d --color",
    "buildProduction": "SET NODE_ENV=production && webpack",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "publish-for-iis": "npm run buildProduction && dotnet publish -c Release",

    "rollup:build": "npm run bundle_vendor && npm run bundle_src && npm run es2015_to_es5_vendor && npm run es2015_to_es5",
    "bundle_vendor": "rollup -c rollup.config.vendor.js",
    "bundle_src": "rollup -c rollup.config.js",
    "es2015_to_es5_vendor": "tsc --out ./wwwroot/rollup/vendor.js --target es5 --allowJs ./wwwroot/rollup/vendor.es2015.js",
    "es2015_to_es5": "tsc --out ./wwwroot/rollup/app.js --target es5 --allowJs ./wwwroot/rollup/app.es2015.js"
  },
  "dependencies": {
    "@angular/animations": "~4.4.4",
    "@angular/common": "~4.4.4",
    "@angular/compiler": "~4.4.4",
    "@angular/core": "~4.4.4",
    "@angular/forms": "~4.4.4",
    "@angular/http": "~4.4.4",
    "@angular/platform-browser": "~4.4.4",
    "@angular/platform-browser-dynamic": "~4.4.4",
    "@angular/router": "~4.4.4",
    "@types/jasmine": "~2.5.53",
    "@types/node": "~8.0.33",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "~3.2.3",
    "babel-polyfill": "~6.26.0",
    "classlist": "~1.0.1",
    "clean-webpack-plugin": "~0.1.17",
    "compression-webpack-plugin": "~1.0.1",
    "css": "2.2.1",
    "css-loader": "0.28.4",
    "es6-shim": "0.35.3",
    "expose-loader": "0.7.3",
    "extract-text-webpack-plugin": "~3.0.2",
    "file-loader": "0.11.2",
    "hammerjs": "~2.0.8",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "~2.30.1",
    "isomorphic-fetch": "2.2.1",
    "json-loader": "0.5.4",
    "node-sass": "~4.5.3",
    "preboot": "4.5.2",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.4.2",
    "sass-loader": "~6.0.6",
    "style-loader": "0.18.2",
    "to-string-loader": "1.1.5",
    "tslint": "~5.8.0",
